    "import numpy as np\n",
    "\n",
    "def factorial(n):\n",
    "  if n == 0:\n",
    "    return 1\n",
    "  else:\n",
    "    return np.prod(np.arange(1, n + 1))\n",
    "\n",
    "import math \n",
    "def calculo_euler (i):\n",
    "  return sum(1/factorial(n) for n in range(0, i+1))\n",
    "print(calculo_euler(9))"
   ]
  }
